{
  "name": "vuejs-challenges",
  "type": "module",
  "private": true,
  "description": "Collection of Vue.js challenges",
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/webfansplz/vuejs-challenges#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webfansplz/vuejs-challenges.git"
  },
  "bugs": {
    "url": "https://github.com/webfansplz/vuejs-challenges/issues"
  },
  "keywords": [
    "Vue.js challenges",
    "Vue.js",
    "challenges",
    "Learn Vue.js"
  ],
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "build": "tsx ./scripts/build.ts",
    "docs:dev": "pnpm run -C docs update && pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "build:all": "pnpm run build && pnpm run docs:build",
    "docs:serve": "pnpm run -C docs serve",
    "lint": "eslint .",
    "lintf": "eslint --fix ."
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.3",
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^16.2.15",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vitest/ui": "^0.17.1",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.55.0",
    "eslint-plugin-format": "^0.1.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^20.0.3",
    "pnpm": "7.3.0",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5",
    "vitest": "^0.17.1",
    "vue": "^3.3.11"
  },
  "stackblitz": {
    "startCommand": "npm run test"
  }
}
